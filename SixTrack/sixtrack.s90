          tdispx(max(ielem,1))  = d(1)
          tdispy(max(ielem,1))  = d(2)
+if .not.collimat.and.bnlelens
        endif
+ei
+ei

      if(ncorru.eq.0) then
          write(lout,10000) nr,typ(:8),tl,p1(1),b1(1),al1(1),g1(1),d(1),&
     &dp(1),c(1),cp(1)
          write(lout,10010) b2(1),al2(1),g2(1)
          write(lout,10030) typ(9:16)

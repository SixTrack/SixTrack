              qwcs(ib3,1)=qwc(1)
              qwcs(ib3,2)=qwc(2)
              qwcs(ib3,3)=qwc(3)

              do i2=1,6
                do j2=1,6
                  tas(ib3,i2,j2)=tasm(i2,j2)
                end do
              end do

  190       continue
          else
            if(idp.eq.1.and.iation.eq.1) then
              ncorruo=ncorru
              ncorru=1
              call clorb(zero)
#ifdef DEBUG
!     call dumpbin('aclorb',1,1)

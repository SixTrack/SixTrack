          xs=xsi(i)
          zs=zsi(i)
#include "include/alignf.f90"
!FOX  CRABAMP4=ED(IX)*ZZ0 ;
             crabfreq=ek(ix)*c1e3
             crabpht4=crabph4(ix)
!FOX  Y(1)=Y(1) + (CRABAMP4)*
!FOX  (CRKVE*CRKVE*CRKVE-(3D0*CRKVE*CIKVE*CIKVE))*C1M6*
!FOX  COS(SIGMDA/(CLIGHT*(E0F/E0))*CRABFREQ*2D0*PI + CRABPHT4)
!FOX  *MTCDA/(ONE+DPDA) ;
!FOX  Y(2)=Y(2) - (CRABAMP4)*

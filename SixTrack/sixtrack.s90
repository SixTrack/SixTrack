      ELSE IF (wire_flagco(ix).eq.-1) THEN
!FOX  DXI=DX*C1M3;
!FOX  DYI=DY*C1M3;
      END IF



!-----------------------------------------------------------------------
! X' -> PX'; Y' -> PY
!FOX  YY(1)=YY(1)*(ONE+DPDA) ;
!FOX  YY(2)=YY(2)*(ONE+DPDA) ;

! 1 SHIFT - see the part of the code were wireda is called ....

      IF (wire_flagco(ix).eq.1) THEN
!FOX  XI=(XX(1)+DX)*C1M3;

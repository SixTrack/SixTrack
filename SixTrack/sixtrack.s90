      endif

!FOX  YY(1)=YY(1)*C1M3;
!FOX  YY(2)=YY(2)*C1M3;

!!FOX  DXI=DX*C1M3;
!!FOX  DYI=DY*C1M3;
      IF (wire_flagco(ix).eq.1) THEN
!FOX  DXI=(DX+XCLO)*C1M3;

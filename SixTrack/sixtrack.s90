        write(lout,10143) sixin_harm,sixin_u0,sixin_phag,qs,sixin_alc
      else
        write(lout,*)
      endif
      if(beam_expflag .eq. 0) then
         if(ibb6d.eq.1) then
            write(lout,                                                 &
     &"(t30,'HIRATA''s 6D BEAM-BEAM ELEMENTS'/t30,30('-')//             &
     &t10,'ELEMENT           #_OF_SLICES    CROSSING_ANGLE',            &
     &'     CROSSING_PLANE     COUPLING_ANGLE'/t10,85('-')/)")

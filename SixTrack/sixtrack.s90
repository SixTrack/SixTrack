        flush(93)
        read(95,err=100,end=100)                                        &
     &(crbinrecs(j),j=1,(crnapxo+1)/2),                                 &
     &(crnumxv(j),j=1,crnapxo),                                         &
     &(crnnumxv(j),j=1,crnapxo),                                        &

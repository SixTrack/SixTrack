      call daprid(rot,1,nd2,mf1)
      call dapri(h4,mf1)
      call dapri(f,mf1)
      call daprid(a2,1,nd2,mf1)
      call daprid(a1,1,nd2,mf1)
      call dapri(h,mf2)

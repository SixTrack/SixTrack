             endif
             im=irm(ix)
             r0a=one
             do k=1,nmz
                izu=izu+1
                aaiv(k,m,i)=(ed(ix)*(ak0(im,k)+zfz(izu)*aka(im,k)))/r0a !hr05
                aai(i,k)=aaiv(k,m,i)
                izu=izu+1
                bbiv(k,m,i)=(ed(ix)*(bk0(im,k)+zfz(izu)*bka(im,k)))/r0a !hr05
                bbi(i,k)=bbiv(k,m,i)
                
                r0a=r0a*r0
             enddo
             
             izu=izu+2*mmul-2*nmz
       !------------------------------------------------------------------------------------
          endif
 150   continue
#ifdef DEBUG

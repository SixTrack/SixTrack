     &              bez(j),int(parbe(j,2)),parbe(j,1),parbe(j,3)
            enddo
         endif
         
      elseif(beam_expflag .eq. 1) then
         write(lout,                                                    &
     &"(t30,'HIRATA''s 6D BEAM-BEAM ELEMENTS'/t30,30('-')//             &
     &t10,'ELEMENT           #_OF_SLICES    XING_ANGLE',                &

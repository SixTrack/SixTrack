      call etallnom(df,nd2,'DF        ')
      rewind mfile
!Eric
      rewind 111
      rewind mf1
      rewind mf2
      rewind mf3
      rewind mf4
      rewind mf5
      call daread(damap,nd2,mfile,zero)
!--Normal Form Analysis
      call mapnorm(damap,f,a2,a1,xy,h,nord1)
      call dainv(a1,nv,a1i,nv)
      call dainv(a2,nv,a2i,nv)

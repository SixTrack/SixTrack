#include "include/kickfho.f90"
#include "include/kickfho.f90"
#include "include/kickfho.f90"
#include "include/kickfxxh.f90"
          case (6)  ! NORMAL DODECAPOL
!FOX  EKK=EKK*C1M12 ;
#include "include/kickfho.f90"
#include "include/kickfho.f90"
#include "include/kickfho.f90"
#include "include/kickfho.f90"
#include "include/kickfxxh.f90"
          case (7)  ! NORMAL 14-POL
!FOX  EKK=EKK*C1M15 ;
#include "include/kickfho.f90"

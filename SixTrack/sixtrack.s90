#ifdef CRLIBM
              call disable_xp()
#endif
#else
#ifndef CRLIBM
              read(33,*) (clo6(l),clop6(l), l=1,3)
#else
              read(33,*) ch 
              lineno=lineno+1
              ch1(:nchars+3)=ch(:nchars)//' / '
              call splitfld(errno,33,lineno,nofields,nf,ch1,fields)
              do l=1,3
                if (nf.gt.0) then
                  clo6(l)=fround(errno,fields,l*2-1)
                  nf=nf-1
                endif
                if (nf.gt.0) then
                  clop6(l)=fround(errno,fields,l*2)
                  nf=nf-1
                endif
              enddo
#endif
#endif
            endif
            call clorb(zero)
            call betalf(zero,qw)
            call phasad(zero,qwc)
            sigm(1)=clo6(3)
            dps(1)=clop6(3)
#ifdef DEBUG

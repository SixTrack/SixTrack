#endif

        !! Initialize kicks
        izu=0
        do 150 i=1,iu
#ifdef DEBUG
!       call warr('i/iu',0d0,i,iu,0,0)
!       write(*,*) 'do 150 i/iu',i,iu
#endif
          ix=ic(i)
          if(ix.le.nblo) goto 150
          ix=ix-nblo
          kpz=kp(ix)
          kzz=kz(ix)
          if(kpz.eq.6.or.kzz.eq.0.or.kzz.eq.20.or.kzz.eq.22) goto 150
          if(kzz.eq.15) goto 150
          if(iorg.lt.0) mzu(i)=izu

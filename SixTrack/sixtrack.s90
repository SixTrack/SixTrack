      goto 330
    case (9) ! NORMAL 18-POLE
      ekk=ekk*c1m21
+ca kicksho
+ca kicksho
+ca kicksho
+ca kicksho
+ca kicksho
+ca kicksho
      if(ium.ne.1) then
+ca kickq09h
      end if
+ca kicksho
+ca kickuxxh

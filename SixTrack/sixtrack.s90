      ELSE IF (wire_flagco(ix).eq.-1) THEN
!FOX  XI=(XX(1)+(DX-XCLO))*C1M3;
!FOX  YI=(XX(2)+(DY-YCLO))*C1M3;
      END IF

! ibeco = 0
      if(ibeco.eq.0) then

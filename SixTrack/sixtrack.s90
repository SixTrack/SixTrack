!     call abend('after bib1                                        ')
#endif
          do 220 ib1=1,napx
            iar=ib1+(m+ib-2)*napx

            do ib2=1,6
              do ib3=1,6
                tau(ib2,ib3)=tas(iar,ib3,ib2)

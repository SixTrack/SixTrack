          al1(l)=-one*(t(ll,ll-1)*t(ll,ll)+t(ll+1,ll-1)*t(ll+1,ll))      !hr06
          al2(l)=-one*(t(6-ll,ll-1)*t(6-ll,ll)+t(7-ll,ll-1)*t(7-ll,ll))  !hr06
          g1(l)=t(ll,ll)**2+t(ll+1,ll)**2                                !hr06
          g2(l)=t(6-ll,ll)**2+t(7-ll,ll)**2                              !hr06
          d(l)=t(6,ll-1)*c1m3

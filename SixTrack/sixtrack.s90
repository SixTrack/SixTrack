        endif
        do 250 ib=1,imc
          if(imc.gt.1) then
            dp1=dp0-(real(ib,fPrec)-one)*ddp1                                  !hr05
          endif
          dp10=dp1
!-----------------------------------------------------------------------
          if(idp.ne.1.or.iation.ne.1) iclo6=0

     &'wire_dispx(',j,') = ',wire_dispx(j),                             &
     &'wire_dispy(',j,') = ',wire_dispy(j),                             &
     &'wire_lint(',j,') = ',wire_lint(j),                               &
     &'wire_lphys(',j,') = ',wire_lphys(j)
            end if

            goto 2501           !Search success :)
            
         endif
      enddo

!     Search for element failed!
      write(lout,*) "ERROR in WIRE: "//                                 &
     &     "Un-identified SINGLE ELEMENT '",                            &

#ifdef DEBUG
!         if (n.eq.1) then
!           write (93,*) 'ktrack(i)=',ktrack(i)
!           endfile (93,iostat=ierro)
!           backspace (93,iostat=ierro)
!         endif
#endif

!          if( bdex_enable .and. kz(ix).eq.0 .and. bdex_elementAction(ix).ne.0 ) then
!             call bdex_track(i,ix,n)
!          end if
#endif

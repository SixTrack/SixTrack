subroutine daliesix
      use floatPrecision
      use end_sixtrack
      use numerical_constants
      use mathlib_bouncer
      use crcoall
      use parpro
      implicit none
      integer i,mf1,mf2,mf3,mf4,mf5,mfile,nd2,ndim,ndpt,nis,no,nv,damap,a1,a1i,a2,a2i,f,fc,fs,rot,xy,h,hc,hs,h4,df,bb1,bb2,haux
